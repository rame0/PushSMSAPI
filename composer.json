{
  "name": "rame0/pushsms-api",
  "type": "library",
  "description": "PUSH SMS API client for PHP",
  "version": "0.0.1",
  "authors": [
    {
      "name": "rame0",
      "email": "r@me0.biz",
      "homepage": "https://github.com/rame0/pushsms-api"
    }
  ],
  "keywords": [
    "api",
    "PushSMS",
    "pushsms.ru"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "autoload": {
    "psr-4": {
      "rame0\\API\\PushSMS\\": "lib/API/PushSMS/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rame0\\API\\PushSMS\\Tests\\": "tests/API/PushSMS/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vlucas/phpdotenv": "^5.5"
  }
}
